openapi: 3.0.3
# note: 3.1.0 required for requestBody on DELETE
# which we should maybe move away from
info:
  title: ML API
  description: The REST API for MACHINE LEARNING 
  license:
    name: BSD-3-Clause
  version: 1.0.0.dev
servers:
  - url: /
# security:
  # - token: []
  # - oauth2:
  #     - self
paths:
  /:
    get: 
      summary: ML API version and info
      description: |
        This endpoint is not authenticated for the purpose of clients and user
        to identify the JupyterHub version before setting up authentication.
      responses:
        200:
          description: The ML API version
          content:
            application/json:
              schema:
                type: object
                properties:
                  version:
                    type: string
                    description: The version of ML API itself
  /action:
      post:
        summary: to fine-tune/predict/call inference endpoint
        requestBody:
          content:
            application/json:
              schema:
                type: object
                properties:
                  command:
                    type: string
                    description: The action to perform, e.g., train, predict, etc.
                    example: train
                  params:
                    type: object
                    description: Configuration parameters for the action
                    properties:
                      project_id:
                        type: integer
                        description: The ID of the project
                        example: 286
                      dataset:
                        type: integer
                        description: Dataset identifier
                        example: 0
                      dataset_version:
                        type: string
                        nullable: true
                        description: Version of the dataset
                        example: null
                      checkpoint:
                        type: string
                        description: Path to the checkpoint
                        example: ""
                      task:
                        type: string
                        description: Machine learning task type
                        example: text-to-image
                      checkpoint_version:
                        type: string
                        nullable: true
                        description: Version of the checkpoint
                        example: null
                      token:
                        type: string
                        description: Authentication token
                        example: "4b89a6ffb1f14bba6148d9167218e062b4d029dc"
                      framework:
                        type: string
                        description: ""
                        example: ""
                      world_size:
                        type: string
                        description: ""
                        example: ""
                      rank:
                        type: string
                        description: ""
                        example: ""
                      master_add:
                        type: string
                        description: ""
                        example: ""
                      master_port:
                        type: string
                        description: ""
                        example: ""
                      host_name:
                        type: string
                        description: ""
                        example: ""
                      wandb_api_key:
                        type: string
                        description: ""
                        example: ""
                      trainingArguments:
                        type: string
                        description: ""
                        example: ""
                      push_to_hub:
                        type: string
                        description: ""
                        example: ""
                      push_to_hub_token:
                        type: string
                        description: ""
                        example: ""
                      channel_log:
                        type: string
                        description: ""
                        example: ""
                    additionalProperties: true
                  project:
                    type: string
                    description: Identifier for the project
                    example: "102"
              examples:
                predict_example:
                  summary: Example of predicting with a model
                  value:
                    command: predict
                    params:
                      text: ""
                      prompt: ""
                      task: "texy-to-image"
                      width: 128,
                      height: 128,
                      num_inference_steps: 50
                      guidance_scale: 1
                      format: "JPEG"
                    project: "102"

                stop train:
                  summary: Example of stop train
                  value:
                    command: action-example
                    project: "102"
                
                train_example:
                  summary: Example of training a model
                  value:
                    command: action-example
                    params:
                      login: null
                      password: null
                      project_id: 286
                      dataset: 0
                      dataset_version: null
                      checkpoint: ""
                      task: text-to-image
                      checkpoint_version: null
                      token: "4b89a6ffb1f14bba6148d9167218e062b4d029dc"
                      rank: 0
                      world_size: 1
                      master_add: "127.0.0.1"
                      master_port: "23456"
                      num_epochs: 1
                      batch_size: 1
                      image_width: "1080"
                      image_height: "1920"
                      imgz: 64
                      workers: null
                      ps: null
                      channel_log: "ml_logs_534"
                      configs: []
                      token_length: "4096"
                      accuracy: "70"
                      precision: "fp16"
                      sampling_frequency: "48000"
                      mono: true
                      fps: "74"
                      resolution: "480"
                      framework: huggingface
                      TrainingArguments:
                        model_id: "black-forest-labs/FLUX.1-dev"
                        load_in_8bit: false
                        load_in_4bit: true
                        strict: false
                        dataset_id: "Norod78/Yarn-art-style"
                        val_set_size: 0.1
                        output_dir: "./outputs/flux-out"
                    project: "237"
        responses:
          200:
            description: The action was successfully performed
          400:
            description: Invalid request parameters
        # security:
        #   - oauth2:
        #       - admin:actions
        x-codegen-request-body-name: body
                
  /model:
    post:
      summary: to interact with the model directly on platform
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                params:
                  type: object
                  description: Configuration parameters for the action
                  properties:
                    login:
                      type: string
                      nullable: true
                      description: Login credentials (optional)
                      example: null
                project:
                  type: string
                  description: Identifier for the project
                  example: "237"
                  
            examples:
                example1:
                  summary: examples1
                  value:
                    params: 
                      task: "text-to-image"
                    project: "102"
               
        required: true
      responses:
        201:
          description: The users have been created

      x-codegen-request-body-name: body
  /model_trial:
    post:
      summary: to interact with the model on gradio
      requestBody:
        content:
          application/json:
            schema:
              type: object
              properties:
                params:
                  type: object
                  description: Configuration parameters for the action
                  properties:
                    login:
                      type: string
                      nullable: true
                      description: Login credentials (optional)
                      example: null
                project:
                  type: string
                  description: Identifier for the project
                  example: "237"
                  
            examples:
                example1:
                  summary: examples1
                  value:
                    params: 
                      task: "text-to-image"
                    project: "237"
               
        required: true
      responses:
        201:
          description: The users have been created

      x-codegen-request-body-name: body
  /downloads:
    get:
      summary: to download model
      parameters:
        - name: path
          in: query
          description: The path file to download
          required: true
          schema:
            type: string
      responses:
        201:
          description: The users have been created
      x-codegen-request-body-name: body
  
  /documents:
    get:
      summary: Get link to the document
      description: Returns the URL to the document (index.html).
      responses:
        '200':
          description: A URL to the document
          
        '404':
          description: File not found
